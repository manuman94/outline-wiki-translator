{
  "name": "outline-translate",
  "version": "1.0.0",
  "description": "Automatically translate Outline Wiki documents to English using OpenAI GPT-4o",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node --watch src/index.ts",
    "list-collections": "ts-node src/listCollections.ts",
    "extract-id": "ts-node src/extractCollectionId.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "outline",
    "translation",
    "openai",
    "typescript",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
